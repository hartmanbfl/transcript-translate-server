{
  "name": "transcript-translate-server",
  "version": "0.0.1",
  "description": "Socket.io based transcription and translation server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "watch": "tsc --watch",
    "nodemon": "nodemon dis/server.js",
    "dev": "concurrently \"yarn:watch\" \"yarn:nodemon\"",
    "devts": "ts-node-dev --respawn --pretty --transpile-only server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "translate": "node dist/server.js ./token.json"
  },
  "author": "Ben Hartman",
  "license": "ISC",
  "dependencies": {
    "@deepgram/sdk": "^3.1.9",
    "@google-cloud/translate": "^8.0.3",
    "@types/google-translate-api": "^2.3.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "google-translate-api-x": "^10.6.7",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^7.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
